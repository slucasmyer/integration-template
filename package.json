{
  "name": "integration-template",
  "version": "1.0.0",
  "description": "Everything.. Everywhere. All at once.",
  "repository": {
    "url": "https://github.com/slucasmyer/integration-template",
    "type": "git"
  },
  "author": "Sullivan Lucas Myer",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test-clean": "jest --clearCache",
    "test-jsdom": "jest --config jest.config.jsdom.ts",
    "test-node": "jest --config jest.config.node.ts",
    "cypress:run": "cypress run --config-file tsconfig.cypress.config.ts",
    "cypress:open": "cypress open --config-file tsconfig.cypress.config.ts",
    "publish": "amplify publish",
    "clean-install": "./scripts/clean-install",
    "commit": "./scripts/commit-script \"$@\"",
    "deploy": "amplify pipeline-deploy --branch $AWS_BRANCH --app-id $AWS_APP_ID",
    "show-vars": "echo Branch: $AWS_BRANCH, App ID: $AWS_APP_ID",
    "build-rust": "rustc --target wasm32-unknown-unknown -O --crate-type=cdylib src/rust_utils.rs -o rust_utils.wasm"
  },
  "dependencies": {
    "@apollo/client": "latest",
    "@apollo/server": "latest",
    "@as-integrations/next": "latest",
    "@aws-sdk/client-dynamodb": "latest",
    "@aws-sdk/lib-dynamodb": "latest",
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@microsoft/microsoft-graph-client": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@popperjs/core": "latest",
    "@prisma/client": "latest",
    "@radix-ui/react-id": "latest",
    "@reduxjs/toolkit": "latest",
    "@stripe/react-stripe-js": "latest",
    "@stripe/stripe-js": "latest",
    "autoprefixer": "latest",
    "aws-amplify": "latest",
    "bufferutil": "latest",
    "graphql": "latest",
    "graphql-redis-subscriptions": "latest",
    "ioredis": "latest",
    "micro": "latest",
    "micro-cors": "latest",
    "next": "latest",
    "next-auth": "latest",
    "nodemailer": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-redux": "latest",
    "stripe": "latest",
    "subscriptions-transport-ws": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "utf-8-validate": "latest",
    "uuid": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@aws-amplify/backend": "latest",
    "@aws-amplify/backend-cli": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@types/jest": "latest",
    "@types/micro-cors": "latest",
    "@types/next": "latest",
    "@types/node": "latest",
    "@types/nodemailer": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-redux": "latest",
    "@types/redux-logger": "latest",
    "@types/uuid": "latest",
    "@yarnpkg/eslint-config": "latest",
    "@yarnpkg/pnpify": "latest",
    "aws-cdk": "latest",
    "aws-cdk-lib": "latest",
    "babel-jest": "latest",
    "constructs": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "prisma": "latest",
    "redux-logger": "latest",
    "ts-jest": "latest",
    "ts-node": "latest"
  },
  "packageManager": "yarn@4.1.1"
}
